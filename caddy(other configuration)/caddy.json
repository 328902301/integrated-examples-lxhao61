          {
            "match": [{
              "host": ["xx.yy"] //限定域名访问（禁止以ip方式访问网站），更改为自己的域名。
            }],
            "handle": [{
              "handler": "headers",
              "response": {
                "set": {
                  "Strict-Transport-Security": ["max-age=31536000; includeSubDomains; preload"] //启用HSTS
                }
              }
            },
            {
              "handler": "file_server",
              "root": "/home/webdav", //更改为自己的文件服务器存放文件路径
              "browse": {} //启用文件列表
            }]
          },
//.........<省略>........
//备注：所有示例的回落或代理网站可参考上边调整为开放式文件服务器。


//二、验证式文件服务器配置
//.........<省略>........
          {
            "match": [{
              "host": ["xx.yy"] //限定域名访问（禁止以ip方式访问网站），更改为自己的域名。
            }],
            "handle": [{
              "handler": "headers",
              "response": {
                "set": {
                  "Strict-Transport-Security": ["max-age=31536000; includeSubDomains; preload"] //启用HSTS
                }
              }
            },
            {
              "handler": "authentication",
              "providers": {
              	"http_basic": {
                  "accounts": [{
                    "username": "user", //身份验证用户
