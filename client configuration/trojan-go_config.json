{
    "run_type": "client",
    "local_addr": "127.0.0.1",
    "local_port": 1080,
    "remote_addr": "xx.yy", //改成服务端配置的对应域名
    "remote_port": 443, //改成服务端配置的对应端口（如没有SNI分流共用端口，就是服务端配置的监听端口。）
    "password": [
        "diy443" //改成服务端设置的对应密码
    ],
    "log_level": 1,
    "ssl": {
        "verify_hostname": true,
        "sni": "",
        "reuse_session": true,
        "session_ticket": false,
        "fingerprint": ""
    },
    "tcp": {
        "no_delay": true,
        "keep_alive": true
    },
    "websocket": {
        "enabled": false, //websocket启用开关。默认关闭，即trojan模式，等同原版trojan使用。
        "path": "", //取决服务端是否设置路径，否则默认（为空）即可。
        "host": "" //若进行CDN流量中转需设置域名，否则默认（为空）即可。
    },
    "shadowsocks": {
        "enabled": false, //shadowsocks AEAD加密启用开关。是否启用取决服务端是否启用（必须与服务器一致），否则无法连接，默认关闭。
        "method": "AES-128-GCM",
        "password": "" //设置密码，必须与服务端一致。
    },
    "mux": {
        "enabled": false, //多路复用启用开关。若服务端设置http/1.1连接可启用，否则关闭。
        "concurrency": 4,
        "idle_timeout": 60
    },
    "router": {
        "enabled": true, //路由策略建议开启。
        "bypass": [
            "geoip:cn",
            "geoip:private",
            "geosite:cn",
            "geosite:private"
        ],
        "block": [
            "geosite:category-ads"
        ],
        "proxy": [
            "geosite:geolocation-!cn"
        ],
        "default_policy": "proxy",
        "geoip": "/trojan-go/geoip.dat", //更改为自己的geoip.dat所在路径
        "geosite": "/trojan-go/geosite.dat" //更改为自己的geosite.dat所在路径
    }
}
