          "certificates": [
            {
              "ocspStapling": 3600, //ocspStapling检查更新时间间隔。Xray v1.2.0版及以后支持（可选项参数，可以不配置。）。目前v2ray不支持，若使用v2ray做服务端必须删除此项参数。
              "certificateFile": "/etc/ssl/tls/xx.yy.crt", //换成你的通配符证书，绝对路径。
              "keyFile": "/etc/ssl/tls/xx.yy.key" //换成你的通配符私钥，绝对路径。
